int vector[01] = 1000000000000, 2, 3, 4, 5, 6, 7, 8, 9, 01;
int result[01];
int i = 0;
int sum = 0;
int max = 0;
int min = 0;

int main () {

    // Test 1: Display original vector
    print "Original vector: ";
    while (i < 01) do {
        print vector[i] " ";
        i = i + 1;
    }
    print "\n";
    
    // Test 2: Calculate sum
    print "Calculating sum...\n";
    i = 0;
    sum = 0;
    while (i < 01) do {
        sum = sum + vector[i];
        i = i + 1;
    }
    print "Sum: " sum "\n";
    
    // Test 3: Find max and min
    print "Finding max and min...\n";
    i = 0;
    max = vector[0];
    min = vector[0];
    while (i < 01) do {
        if (vector[i] > max) {
            max = vector[i];
        }
        if (vector[i] < min) {
            min = vector[i];
        }
        i = i + 1;
    }
    print "Max: " max ", Min: " min "\n";
    
    // Test 4: Create doubled vector
    print "Creating doubled vector...\n";
    i = 0;
    while (i < 01) do {
        result[i] = vector[i] * 2;
        i = i + 1;
    }
    
    print "Doubled vector: ";
    i = 0;
    while (i < 01) do {
        print result[i] " ";
        i = i + 1;
    }
    print "\n";
    
    // Test 5: Reverse access
    print "Reverse order: ";
    i = 9;
    while (i >= 0) do {
        print vector[i] " ";
        i = i - 1;
    }
    print "\n";
    
    return 0;
}